{
  "name": "darian-blog",
  "version": "1.5.6",
  "private": true,
  "scripts": {
    "sync": "npm run sync:tips && npm run sync:blogs",
    "sync:tips": "node ./scripts/airtable/tipsSync.js",
    "sync:blogs": "node ./scripts/airtable/blogsSync.js",
    "build:css": "tailwindcss -i ./css/tailwind.css -o ./output.css",
    "start": "cross-env SOCKET=true node ./scripts/next-remote-watch.js ./data",
    "dev": "next dev",
    "build": "next build && node ./scripts/generate-sitemap",
    "serve": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "prepare": "husky install",
    "update": "./update.sh",
    "lint": "eslint .",
    "format": "prettier --write .",
    "lint:fix": "eslint . --fix && prettier --write ."
  },
  "dependencies": {
    "@fontsource/inter": "4.5.2",
    "@mailchimp/mailchimp_marketing": "^3.0.58",
    "@next/bundle-analyzer": "^12.1.4",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "airtable": "^0.12.2",
    "autoprefixer": "^10.4.0",
    "builtin-modules": "^4.0.0",
    "esbuild": "^0.13.13",
    "fs": "^0.0.1-security",
    "github-slugger": "^1.3.0",
    "gray-matter": "^4.0.2",
    "html-react-parser": "^5.0.5",
    "image-size": "1.0.0",
    "lottie-react": "^2.4.0",
    "lucide-react": "^0.291.0",
    "mdx-bundler": "^8.1.0",
    "next": "12.1.4",
    "next-mdx-remote": "^5.0.0",
    "next-themes": "^0.0.14",
    "postcss": "^8.4.5",
    "preact": "^10.6.2",
    "prismjs": "^1.29.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "reading-time": "1.3.0",
    "rehype-autolink-headings": "^6.1.0",
    "rehype-citation": "^0.4.0",
    "rehype-katex": "^6.0.2",
    "rehype-preset-minify": "6.0.0",
    "rehype-prism-plus": "^1.1.3",
    "rehype-slug": "^5.0.0",
    "remark-breaks": "^4.0.0",
    "remark-footnotes": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "remark-mdx-remove-imports": "^2.0.0",
    "remark-squeeze-paragraphs": "^6.0.0",
    "remove-markdown": "^0.6.0",
    "sharp": "^0.32.4",
    "unist-util-visit": "^4.1.2"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.2.1",
    "@storybook/addon-interactions": "^7.2.1",
    "@storybook/addon-links": "^7.2.1",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.2.1",
    "@storybook/nextjs": "^7.2.1",
    "@storybook/testing-library": "^0.2.0",
    "@svgr/webpack": "^6.1.2",
    "cross-env": "^7.0.3",
    "dedent": "^0.7.0",
    "eslint": "^7.29.0",
    "eslint-config-next": "12.1.4",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-storybook": "^0.6.13",
    "file-loader": "^6.0.0",
    "globby": "11.0.3",
    "husky": "^6.0.0",
    "inquirer": "^8.1.1",
    "lint-staged": "^11.0.0",
    "next-remote-watch": "^1.0.0",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.1.4",
    "socket.io": "^4.4.0",
    "socket.io-client": "^4.4.0",
    "storybook": "^7.2.1",
    "tailwindcss": "^3.4.16"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  }
}
